{
  "manifest_version": 2,

  "name": "Focus.Me",
  "description": "Focus.me is a pomodoro timer",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Focus.Me"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
